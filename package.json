		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"repository": "https://github.com/DougAnderson444/svelte-resizable",
