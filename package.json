{
	"name": "@douganderson444/svelte-resizable",
	"version": "0.0.6",
	"module": "index.js",
	"svelte": "index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package",
		"publish:npm": "npm publish ./package --access public",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^1.0.0-next.6",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.5.10",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"repository": "https://github.com/DougAnderson444/svelte-resizable",
	"dependencies": {
		"@douganderson444/pointer-tracker": "github:DougAnderson444/pointer-tracker"
	}
}
